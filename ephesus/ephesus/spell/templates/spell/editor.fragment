{% extends "spell/editor-pane.html" %}{% block editor %}
{% if verses|length == 0 %}
<p>No data found.</p>
{% else %}
<div>
  <h2>{{ ref }}</h2>
  {% for verse in verses %}
  <div>
    <span class="bold verse-number">{{ verse[0] }}</span>
    <span data-ref="{{ ref }}"
          class="verse" contenteditable="true">{{ verse[1] }}</span>
  </div>
  {% endfor %}
</div>
{% endif %}
{% endblock %}
